{
  "version": 2,
  "name": "hill-street-run-club",
  "buildCommand": "cd website && npm run build",
  "outputDirectory": "website/.next",
  "framework": "nextjs",
  "installCommand": "cd website && npm install",
  "devCommand": "cd website && npm run dev",
  "rootDirectory": "./website",
  "functions": {
    "website/src/app/api/**": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
